{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "common jest",
            "program": "${workspaceFolder}/common/node_modules/jest/bin/jest",
            "args": [
                "--runInBand"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeArgs": [
                "--inspect-brk=9229"
            ],
            "port": 9229,
            "sourceMaps": true,
            "cwd": "${workspaceFolder}/common",
            "windows": {
                "program": "${workspaceFolder}/common/node_modules/jest/bin/jest.cmd"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "backend jest",
            "program": "${workspaceFolder}/backend/node_modules/jest/bin/jest",
            "args": [
                "--runInBand"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeArgs": [
                "--inspect-brk=9229"
            ],
            "port": 9229,
            "sourceMaps": true,
            "cwd": "${workspaceFolder}/backend",
            "windows": {
                "program": "${workspaceFolder}/backend/node_modules/jest/bin/jest.cmd"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "frontend jest",
            "program": "${workspaceFolder}/frontend/node_modules/jest/bin/jest",
            "args": [
                "--runInBand"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeArgs": [
                "--inspect-brk=9229"
            ],
            "port": 9229,
            "sourceMaps": true,
            "cwd": "${workspaceFolder}/frontend",
            "windows": {
                "program": "${workspaceFolder}/frontend/node_modules/jest/bin/jest.cmd"
            }
        }
   ]
}